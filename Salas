package atividade1;

public enum Salas {
	scomum("comum", 1.0),
	s3D("3D", 1.2),
	sXD("XD", 1.3),
	sXDe3D("XD/D", 1.4);
	
	private String tipo;
	private double multiplicador;
	
	private Salas(String tipo, double multiplicador) {
		this.tipo = tipo;
		this.multiplicador = multiplicador;
	}
	
	public double getMultiplicador() {
		return multiplicador;
	}
	
}
